{
  "name": "porto",
  "description": "Next-gen Account for Ethereum",
  "version": "0.0.1",
  "type": "module",
  "main": "./_dist/index.js",
  "types": "./_dist/index.d.ts",
  "typings": "./_dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./_dist/index.d.ts",
      "default": "./_dist/index.js"
    },
    "./Chains": {
      "types": "./_dist/Chains.d.ts",
      "default": "./_dist/Chains.js"
    },
    "./Porto": {
      "types": "./_dist/Porto.d.ts",
      "default": "./_dist/Porto.js"
    },
    "./wagmi": {
      "types": "./_dist/wagmi.d.ts",
      "default": "./_dist/wagmi.js"
    },
    "./wagmi/actions": {
      "types": "./_dist/actions.d.ts",
      "default": "./_dist/actions.js"
    },
    "./wagmi/query": {
      "types": "./_dist/query.d.ts",
      "default": "./_dist/query.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "Chains": ["./_dist/Chains.d.ts"],
      "Porto": ["./_dist/Porto.d.ts"],
      "wagmi": ["./_dist/wagmi.d.ts"],
      "wagmi/actions": ["./_dist/actions.d.ts"],
      "wagmi/query": ["./_dist/query.d.ts"]
    }
  },
  "files": [
    "*",
    "!**/_test/**",
    "!**/*.tsbuildinfo",
    "!tsconfig.build.json",
    "!jsr.json",
    "/Chains",
    "/Porto",
    "/wagmi"
  ],
  "dependencies": {
    "idb-keyval": "^6.2.1",
    "mipd": "^0.0.7",
    "ox": "^0.2.2",
    "zustand": "^5.0.1"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=5.59.0",
    "react": ">=18",
    "typescript": ">=5.4.0",
    "viem": ">=2.0.0",
    "wagmi": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "wagmi": {
      "optional": true
    }
  },
  "license": "MIT",
  "homepage": "https://ithaca.xyz",
  "repository": "ithacaxyz/porto",
  "contributors": ["jxom.eth <jake@ithaca.xyz>", "awkweb.eth <tom@ithaca.xyz>"]
}
