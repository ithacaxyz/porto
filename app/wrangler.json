{
  "$schema": "https://esm.sh/wrangler/config-schema.json",
  "minify": true,
  "name": "wallet",
  "keep_vars": true,
  "workers_dev": true,
  "compatibility_date": "2025-02-26",
  "compatibility_flags": ["nodejs_als", "nodejs_compat"],
  "dev": {
    "port": 6901
  },
  "placement": {
    "mode": "smart"
  },
  "observability": {
    "enabled": true
  },
  "assets": {
    "directory": "./dist"
  },
  "vars": {
    "VITE_LAUNCH_MODE": "app"
  }
}
