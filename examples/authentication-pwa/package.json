    "react-dom": "^19.1",
    "viem": "^2.37.8",
    "wagmi": "^2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.13",
    "@cloudflare/workers-types": "^4.20251008.0",
    "@types/node": "^22",
    "@types/react": "^19.1",
    "@types/react-dom": "^19.1",
    "@vite-pwa/assets-generator": "^1.0.2",
    "@vitejs/plugin-react": "^5.0.4",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "vite": "^6.3.6",
    "vite-plugin-mkcert": "^1.17.9",
    "vite-plugin-pwa": "^1.1.0",
