{
  "name": "react-native-relay-mode",
  "main": "index.ts",
  "scripts": {
    "dev": "expo start --dev-client --tunnel --clear",
    "dev:ios": "expo start --ios --clear",
    "dev:android": "expo start --android --clear",
    "web": "pnpm dev",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "ios-no-cache": "expo run:ios --no-build-cache",
    "android-no-cache": "expo run:android --no-build-cache",
    "dev:server": "bun --watch --hot ./server/index.ts --env-file='.env'",
    "doctor": "pnpm dlx expo-doctor@latest --verbose --pnpm && pnpm expo install --fix --pnpm"
  },
  "dependencies": {
    "expo": "54.0.20",
    "expo-auth-session": "~7.0.8",
    "expo-build-properties": "~1.0.9",
    "expo-checkbox": "~5.0.7",
    "expo-crypto": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-network": "~8.0.7",
    "expo-web-browser": "~15.0.8",
    "ox": "^0.9.12",
    "porto": "workspace:*",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-passkeys": "^0.4.0",
    "react-native-safe-area-context": "~5.6.1",
    "react-native-web": "^0.21.2",
    "viem": "^2.38.3",
    "wagmi": "^2.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@types/babel__core": "^7.20.5",
    "@types/bun": "^1.3.1",
    "@types/react": "~19.1.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "expo-dev-client": "~6.0.15",
    "typescript": "~5.9.3"
  },
  "private": true
}
