{
  "name": "better-auth-example",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:types": "wrangler types --include-runtime=false",
    "db:init": "pnpx @better-auth/cli generate --config ./better-auth.cli.ts --output ./migrations/0_init.sql && wrangler d1 execute example --file migrations/0_init.sql",
    "deploy": "vite build && wrangler deploy",
    "dev": "vite",
    "prepreview": "pnpm build:types",
    "preview": "vite build && vite preview"
  },
  "dependencies": {
    "@tanstack/react-query": "^5",
    "better-auth": "^1.3.28",
    "better-sqlite3": "^12.4.1",
    "hono": "^4.10.3",
    "kysely": "^0.28.8",
    "kysely-d1": "^0.4.0",
    "porto": "latest",
    "react": "^19.1",
    "react-dom": "^19.1",
    "viem": "^2.37.8",
    "wagmi": "^2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.13",
    "@cloudflare/workers-types": "^4.20251014.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^19.1",
    "@types/react-dom": "^19.1",
    "@vitejs/plugin-react": "^5.0.4",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "vite": "^6.3.6",
    "vite-plugin-mkcert": "^1.17.9",
    "wrangler": "^4.43.0"
  }
}
